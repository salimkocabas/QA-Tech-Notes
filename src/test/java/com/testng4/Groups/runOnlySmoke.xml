<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<test thread-count="5" name="runOnlySmoke">




		<!-- you can tag test methods in relevant class which are runs, only works 
			that attribute -->
		<groups>
			<run>
				<include name="Smoke" />
			</run>
		</groups>

		<packages>
			<package name="com.testng4.Groups" />
		</packages>


	</test> <!-- Test -->
</suite> <!-- Suite -->
